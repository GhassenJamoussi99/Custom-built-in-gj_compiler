// tests comparisions/if statements
x:integer = 5;
main: function integer () =
{
	y:integer = 10;
	z:integer = 5;
	if(x >= z)
	{
		print "x \>= z\n";
	}
	else
	{
		print "shouldn't be printed\n";
	}

	if(y > x )
	{
		print "y \> x\n";
	}
	else
	{
		print "shouldn't be printed\n";
	}
	
	if(z == x )
	{
		print "z == x\n";
	}
	else
	{
		print "shouldn't be printed\n";
	}

	if(x <= y )
	{
		print "x \<= y\n";
	}
	else
	{
		print "shouldn't be printed\n";
	}

	if(x != y )
	{
		print "x != y\n";
	}
	else
	{
		print "shouldn't be printed\n";
	}

	return 0;
}
